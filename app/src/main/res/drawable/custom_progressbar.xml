<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"></corners>
            <gradient
                android:angle="270"
                android:centerColor="#C0B9B9"
                android:centerY="1.0"
                android:endColor="#C0B9B9"
                android:startColor="#C0B9B9"></gradient>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"></corners>
                <gradient
                    android:angle="270"
                    android:centerColor="#007A00"
                    android:centerY="1.0"
                    android:endColor="#06101d"
                    android:startColor="#007A00"></gradient>
            </shape>
        </clip>
    </item>
</layer-list>